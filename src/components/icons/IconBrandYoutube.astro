---
interface Props {
  classes?: string;
  height: number;
  width: number;
}

const { classes, height, width } = Astro.props;
---

<svg 
  xmlns="http://www.w3.org/2000/svg" 
  width={width} 
  height={height} 
  viewBox="0 0 24 24" 
  fill="none" 
  stroke="currentColor" 
  stroke-width="1.25" 
  stroke-linecap="round" 
  stroke-linejoin="round" 
  class={"w-auto h-auto stroke-current" + {classes}}
>
