---
interface Props {
  classes?: string;
  height: number;
  width: number;
}

const { classes, height, width } = Astro.props;
---

<svg 
  xmlns="http://www.w3.org/2000/svg" 
  width={width} 
  height={height} 
  viewBox="0 0 24 24" 
  fill="none" 
  stroke="currentColor" 
  stroke-width="1.25" 
  stroke-linecap="round" 
  stroke-linejoin="round" 
  class={`w-auto h-auto stroke-current ${classes}`}
>
  <path stroke="none" d="M0 0h24v24H0z" fill="none" />
  <path d="M17 7l-10 10" />
  <path d="M8 7l9 0l0 9" />
</svg>
